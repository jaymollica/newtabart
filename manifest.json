{
  "name": "New Tab Art",
  "version": "0.1",
  "description": "See a new piece of art every time you open a new tab.",
  "manifest_version": 2,
  "icons": {},
  "background": {
    "scripts": ["background.js"],
    "matches": ["http://*/*", "https://*/*"],
    "persistent": false
  },
  "browser_action": {
    "default_title":""
  },
   "content_scripts": [
    {
        "matches": [ "http://*/*", "https://*/*", "file:///*/*"],
        "js": ["jquery-3.4.1.min.js","getart.js"],
        "all_frames": true
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newpage.html"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*", "file:///*/*"],
  "web_accessible_resources": ["style.css"]
}